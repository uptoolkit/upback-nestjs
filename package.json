{
  "name": "upback-nestjs",
  "version": "0.0.1",
  "private": true,
  "description": "Up And Running Nest JS Boilerplate with Swagger and GraphQL",
  "license": "ISC",
  "author": "Daniel Sum",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "db:generate": "prisma generate",
    "db:seed": "prisma seed",
    "db:reset": "prisma migrate reset",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@nestjs/apollo": "^13.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "11.0.12",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "11.0.12",
    "@nestjs/graphql": "^13.0.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^11.0.9",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "11.0.12",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "graphql": "^16.10.0",
    "graphql-scalars": "1.24.1",
    "graphql-tools": "9.0.13",
    "graphql-type-json": "^0.3.2",
    "hbs": "^4.2.0",
    "helmet": "^8.0.0",
    "mysql2": "^3.12.0",
    "nestjs-prisma-crud": "^1.0.0-beta.18",
    "nestjs-prisma-crud-schematics": "^1.0.0-beta.12",
    "nestjs-supabase-auth": "^1.0.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma-graphql-type-decimal": "^3.0.0",
    "query-string": "^9.1.1",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.1",
    "ts-morph": "^25.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@nestjs/cli": "11.0.5",
    "@nestjs/schematics": "11.0.2",
    "@nestjs/testing": "11.0.12",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.1",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.24.0",
    "@typescript-eslint/parser": "8.24.0",
    "eslint": "9.20.1",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "jest": "29.7.0",
    "prettier": "3.5.0",
    "prisma": "6.3.1",
    "prisma-nestjs-graphql": "^21.1.1",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
