{
  "name": "upback-nestjs",
  "version": "0.0.1",
  "private": true,
  "description": "Up And Running Nest JS Boilerplate with Swagger and GraphQL",
  "license": "ISC",
  "author": "Daniel Sum",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "db:generate": "prisma generate",
    "db:seed": "prisma seed",
    "db:reset": "prisma migrate reset",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "8.4.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "8.4.7",
    "@nestjs/graphql": "^10.1.2",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^9.1.2",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/terminus": "^9.1.1",
    "@nestjs/throttler": "^3.0.0",
    "@prisma/client": "^4.3.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "apollo-server-express": "^3.10.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "graphql": "^16.6.0",
    "graphql-scalars": "1.17.0",
    "graphql-tools": "8.3.0",
    "graphql-type-json": "^0.3.2",
    "hbs": "^4.2.0",
    "helmet": "^6.0.0",
    "mysql2": "^2.3.3",
    "nestjs-prisma-crud": "^1.0.0-beta.16",
    "nestjs-prisma-crud-schematics": "^1.0.0-beta.12",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prisma-graphql-type-decimal": "^2.0.4",
    "query-string": "^7.1.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "ts-morph": "^16.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@nestjs/cli": "8.2.8",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.7",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.3",
    "@types/node": "16.11.42",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.2",
    "prettier": "2.7.1",
    "prisma": "4.3.1",
    "prisma-nestjs-graphql": "^17.0.1",
    "supertest": "6.2.3",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
